{
    "name": "gourmet-guide",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "format": "concurrently -c \"#BC507A\",\"#7963E6\",\"#7A8387\" -n prettier,eslint,stylelint \"prettier --ignore-path \\\"./.gitignore\\\" --write \\\"**/*.{js,jsx,ts,html,pug,css,pcss,sass,scss,json}\\\"\" \"eslint --cache --ignore-path \\\"./.gitignore\\\" --fix \\\"**/*.{js,jsx,ts}\\\"\" \"stylelint --cache --ignore-path \\\"./.gitignore\\\" --fix \\\"**/*.css\\\"\"",
        "lint": "concurrently -c \"#7963E6\",\"#7A8387\" -n eslint,stylelint \"eslint --cache --ignore-path \\\"./.gitignore\\\" \\\"**/*.{js,jsx,ts}\\\"\" \"stylelint --cache --ignore-path \\\"./.gitignore\\\" \\\"**/*.css\\\"\""
    },
    "lint-staged": {
        "*.{js,jsx,ts,html,css,json}": "prettier --write --ignore-path \"./.gitignore\"",
        "*.{js,jsx,ts,tsx}": "eslint --cache --ignore-path \"./.gitignore\"",
        "*.css": "stylelint --cache --fix --ignore-path \"./.gitignore\""
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.30.6",
        "@vitejs/plugin-react": "^2.0.0",
        "autoprefixer": "^10.4.7",
        "concurrently": "^7.2.2",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-tailwindcss": "^3.6.0",
        "eslint-webpack-plugin": "^3.2.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "postcss": "^8.4.14",
        "prettier": "^2.7.1",
        "stylelint": "^14.9.1",
        "stylelint-config-idiomatic-order": "^8.1.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recommended": "^8.0.0",
        "stylelint-config-standard": "^26.0.0",
        "tailwindcss": "^3.1.6",
        "typescript": "^4.6.4",
        "vite": "^3.0.0"
    }
}
